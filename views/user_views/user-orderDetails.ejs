    <script>
        // cancel order
        const cancelButton = document.getElementById("cancelButton");
    
        cancelButton.addEventListener("click", function (e) {
            e.preventDefault();
    
            const orderId = cancelButton.getAttribute("order-id");
    
            // Show SweetAlert with input for reason
            Swal.fire({
                title: 'Cancel Order',
                input: 'select',
                inputOptions: {
                    'Cancel Reason: bad_product': 'Bad Product',
                    'Cancel Reason: price_decreased': 'Price Decreased',
                    'Cancel Reason: quality_issues': 'Quality Issues'
                },
                inputPlaceholder: 'Select a reason',
                showCancelButton: true,
                confirmButtonColor: '#d33',
                cancelButtonColor: '#3085d6',
                confirmButtonText: 'Cancel Order'
            }).then((result) => {
                if (result.isConfirmed) {
                    const selectedReason = result.value;
    
                    // Check if a reason is selected
                    if (selectedReason) {
                        // Send the request with the selected reason
                        fetch(`/cancel-order?orderId=${orderId}&reason=${selectedReason}`, {
                            method: 'POST',
                            headers: {
                                'Content-Type': 'application/json',
                            },
                        })
                            .then(response => {
                                if (!response.ok) {
                                    throw new Error(`HTTP error! status: ${response.status}`);
                                }
                                return response.json();
                            })
                            .then(data => {
                                console.log("Response from server", data);
    
                                if (data.message) {
                                    // Reload the page or perform any other action
                                    window.location.reload();
                                }
                            })
                            .catch(error => {
                                console.error("Error cancelling product", error);
                            });
                    } else {
                        // User did not select a reason, show an error message or handle it accordingly
                        Swal.fire({
                            icon: 'error',
                            title: 'Oops...',
                            text: 'Please select a cancel reason!',
                        });
                    }
                }
            });
        });
    </script>

<script>
    // Function to handle the SweetAlert confirmation
    function confirmCancellation(productId, orderId) {
        Swal.fire({
            title: 'Are you sure?',
            text: 'You are about to cancel this product.',
            icon: 'warning',
            showCancelButton: true,
            confirmButtonColor: '#3085d6',
            cancelButtonColor: '#d33',
            confirmButtonText: 'Yes, cancel it!'
        }).then((result) => {
            if (result.isConfirmed) {
                cancelProduct(productId, orderId);
            }
        });
    }

    // Function to handle the cancellation logic
    function cancelProduct(productId, orderId) {
        // Perform the cancellation logic using fetch or any other method
        fetch(`/cancelSingle-order?orderId=${orderId}&productId=${productId}`, {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json',
            },
        })
        .then(response => {
            if (!response.ok) {
                throw new Error(`HTTP error! status: ${response.status}`);
            }
            return response.json();
        })
        .then(data => {
            console.log("Response from server", data);

            if (data.message) {
                // Reload the page or perform any other action
                window.location.reload();
            }
        })
        .catch(error => {
            console.error('Error:', error);
        });
    }

    // Cancel single product
    const cancelProductBtns = document.querySelectorAll(".cancelProduct-Btn");

    cancelProductBtns.forEach(button => {
        button.addEventListener("click", function (e) {
            e.preventDefault();

            const productId = button.getAttribute("product-id");
            const orderId = button.getAttribute("order-id");

            // Show SweetAlert confirmation
            confirmCancellation(productId, orderId);
        });
    });
</script>

